{
    "name": "siganushka/user-bundle",
    "description": "User Bundle.",
    "keywords": ["user-bundle"],
    "homepage": "https://github.com/siganushka/user-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "knplabs/knp-components": "^5.0",
        "siganushka/doctrine-contracts": "dev-main",
        "siganushka/generic-bundle": "dev-main",
        "symfony/doctrine-bridge": "^7.4|^8.0",
        "symfony/form": "^7.4|^8.0",
        "symfony/framework-bundle": "^7.4|^8.0",
        "symfony/security-core": "^7.4|^8.0",
        "symfony/validator": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\UserBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tools:run": [
            "tools/vendor/bin/php-cs-fixer fix -vvv",
            "tools/vendor/bin/simple-phpunit -vvv",
            "tools/vendor/bin/phpstan analyse -vvv"
        ]
    }
}
